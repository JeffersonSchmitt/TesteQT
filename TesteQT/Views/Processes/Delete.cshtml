@model TesteQT.Models.Process

@{
    ViewData["Title"] = "Delete";
}

<h1>Deseja realmente deletar este processo?</h1>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Codigo do processo
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ProcessCode)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a style="margin-left:15px" class="btn btn-primary" asp-action="Index">Voltar ao inicio</a>
    </form>
</div>
